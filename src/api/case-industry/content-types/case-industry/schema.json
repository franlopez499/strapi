  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true,
      "maxLength": 100
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "description": {
      "type": "text",
      "required": false,
      "maxLength": 500
    },
    "icon": {
      "type": "string",
      "required": false,
      "maxLength": 50
    },
    "color": {
      "type": "string",
      "required": false,
      "default": "bg-blue-100 text-blue-800"
    },
    "case_studies": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::case-study.case-study",
      "mappedBy": "industry"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    }
  }
