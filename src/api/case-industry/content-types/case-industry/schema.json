{
  "kind": "collectionType",
  "collectionName": "case_industries",
  "info": {
    "singularName": "case-industry",
    "pluralName": "case-industries",
    "displayName": "Case Industry",
    "description": "Sectores industriales para los casos de Ã©xito"
