{
  "kind": "collectionType",
  "collectionName": "case_services",
  "info": {
    "singularName": "case-service",
    "pluralName": "case-services",
    "displayName": "Case Service",
    "description": "Servicios ofrecidos en los casos de Ã©xito"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true,
      "maxLength": 100
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "description": {
      "type": "text",
      "required": false,
      "maxLength": 500
    },
    "icon": {
      "type": "string",
      "required": false,
      "maxLength": 50
    },
    "color": {
      "type": "string",
      "required": false,
      "default": "bg-green-100 text-green-800"
    },
    "case_studies": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::case-study.case-study",
      "inversedBy": "services"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    }
  }
}
